<% include ../partials/header %>



<div class="ui container">
        <div class="row">          
            </div>
            <div class="col-md-9">
                <div class="thumbnail">
                    <img class="img-responsive" src="<%= doc.image %>">
                    <div class="caption-full">
                        <h4 class="pull-right"></h4>
                        <h4><a><%=doc.name%></a></h4>
                        <p><%= doc.description %></p>
                    </div>
                </div>
                    
                
                <div class="well">
                    <div class="text-right">
                        <a class="btn btn-success" href="/docs/<%= doc._id %>/comments/new">Add New Comment</a>
                    </div>
                    <hr>
                    <% doc.comments.forEach(function(comment){ %>
                        <div class="row">
                            <div class="col-md-12">
                                <strong><%= comment.author %></strong>
                                <span class="pull-right">10 days ago</span>
                                <p>
                                    <%= comment.text %> 
                                </p>
                            </div>
                        </div>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>
    

<div class="ui container">


        <h2 id="schedule-heading">Agenda</h2>
        <div class="schedule" aria-labelledby="schedule-heading">
            
            <span class="track-slot" aria-hidden="true" style="grid-column: track-1; grid-row: tracks;">Montag</span>
            <span class="track-slot" aria-hidden="true" style="grid-column: track-2; grid-row: tracks;">Dienstag</span>
            <span class="track-slot" aria-hidden="true" style="grid-column: track-3; grid-row: tracks;">Mittwoch</span>
            <span class="track-slot" aria-hidden="true" style="grid-column: track-4; grid-row: tracks;">Donnerstag</span>
            <span class="track-slot" aria-hidden="true" style="grid-column: track-5; grid-row: tracks;">Freitag</span>    
